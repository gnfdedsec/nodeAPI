{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users",
        "dest": "/index.js"
      },
      {
        "src": "/api/user/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/",
        "dest": "/index.js" // เส้นทางหลัก
      }
    ]
  }
  